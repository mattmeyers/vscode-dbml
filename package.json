{
	"name": "vscode-dbml",
	"displayName": "vscode-dbml",
	"description": "Language support for the Database Markup Language",
	"icon": "images/dbml-logo.png",
	"version": "0.4.1",
	"publisher": "matt-meyers",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mattmeyers/vscode-dbml.git"
	},
	"activationEvents": [
		"onCommand:vscode-dbml.export",
		"onCommand:vscode-dbml.import"
	],
	"main": "./dist/main.js",
	"contributes": {
		"languages": [
			{
				"id": "dbml",
				"aliases": [
					"DBML",
					"dbml",
					"Database Markup Language"
				],
				"extensions": [
					".dbml"
				],
				"icon": {
					"dark": "./icons/dbml-logo.svg",
					"light": "./icons/dbml-logo.svg"
				},
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dbml",
				"scopeName": "source.dbml",
				"path": "./syntaxes/dbml.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "dbml",
				"path": "./snippets/dbml.json"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "vscode-dbml.export",
					"when": "editorLangId == dbml"
				},
				{
					"command": "vscode-dbml.import",
					"when": "editorLangId == sql"
				}
			]
		},
		"commands": [
			{
				"command": "vscode-dbml.export",
				"title": "To SQL",
				"category": "DBML"
			},
			{
				"command": "vscode-dbml.import",
				"title": "From SQL",
				"category": "DBML"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.38",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"ts-loader": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@dbml/core": "^2.0.1"
	},
	"__metadata": {
		"id": "4cea5698-223c-41b3-809b-a904d516f061",
		"publisherDisplayName": "Matt Meyers",
		"publisherId": "568bceb1-d747-4b35-8b70-2913492d364a"
	}
}
